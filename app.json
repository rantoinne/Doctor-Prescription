{
  "expo": {
    "name": "Health Safe Patient",
    "slug": "HealthSafe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hsp.healthsage",
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "com.hsp.healthsage",
      "versionCode": 13,
      "enableDangerousExperimentalLeanBuilds": true
    }
  }
}
